{
	"applications": {
		"gecko": {
			"id": "livenotifier_dev@zatsunenomokou.eu",
			"update_url": "https://gitlab.com/ZatsuneNoMokou/live_notifier/raw/master/dist/live_notifier_dev.update.json",
			"strict_min_version": "52.0"
		}
	},
	"manifest_version": 2,
	"name": "Live Notifier (Dev)",
	"short_name": "LiveNotifierDev",
	"description": "__MSG_extShortDesc__",
	"author": "Zatsune No Mokou",
	"version": "11.0.9",
	"icons": {
		"96": "/icon_96.png",
		"128": "/icon_128.png"
	},
	"background": {
		"scripts": [
			"/data/js/lib/browser-polyfill.min.js",
			"/data/js/chrome-notification.js",
			"/data/js/options-data.js",
			"/data/js/chrome-preferences.js",
			"/data/js/loadJS.js",
			"/data/js/options-api.js",
			"/data/js/voiceAPI.js",
			"/data/js/lib/xml2jsobj.js",
			"/data/js/queue.js",
			"index.js"
		]
	},
	"browser_action": {
		"browser_style": false,
		"default_icon": {
			"16": "/icon_16.png",
			"48": "/icon_48.png",
			"96": "/icon_96.png",
			"128": "/icon_128.png"
		},
		"default_popup": "/data/panel.html"
	},
	"default_locale": "en",
	"offline_enabled": false,
	"permissions": [
		"tabs",
		"activeTab",
		"contextMenus",
		"notifications",
		"storage",
		"unlimitedStorage",
		"http://*/*",
		"https://*/*"
	],
	"optional_permissions": [
		"clipboardWrite"
	],
	"options_ui": {
		"page": "data/options.html",
		"chrome_style": false
	},
	"minimum_chrome_version": "52.0"
}
